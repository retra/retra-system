<?xml version="1.0"?>
<cloudbees-web-app xmlns="http://www.cloudbees.com/xml/webapp/1">
	<!-- Application ID (formatted CB_ACCOUNT/APPNAME) -->
	<appid>retra/retra</appid>

	<!-- Extra context parameters -->
	<context-param>
		<param-name>retra.config.location</param-name>
		<param-value>classpath:/WEB-INF/defaultconf/</param-value>
	</context-param>

	<resource name="RetraDS" auth="Container" type="javax.sql.DataSource">
		<param name="username" value="retra" />
		<param name="password" value="retra" />
		<param name="url" value="jdbc:cloudbees://retra" />
	</resource>
</cloudbees-web-app>


